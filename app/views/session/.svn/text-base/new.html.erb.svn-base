<div class="blackbar" style="padding: 15px;">
	<span class="calibri_16_gray"><b>Please sign in to continue</b></span>
</div>
<div class="contentsbar">
	<table cellspacing=0 cellpadding=0 width="900px;">
		<tr>
			<td width="274px;">
				<div class="login_bk">
					<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
					<div class="field">
				  		<span style="padding-bottom:5px;"><%= f.label :email %></span>
				  		<%= f.email_field :email, :class=>"input_text"  %>
					</div>
					<div class="field">
				  		<%= f.label :password %><br />
				  		<%= f.password_field :password, :class=>"input_text"  %>
					</div> 
					<div class="field">
				  		<button type="submit" class="bluebtn" id="login">Login</button>
					</div>  
					<div class="field">
						<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  							<%= link_to "Forgot your password?", new_password_path(resource_name) %>
						<% end -%>
					</div>
				<% end %>
			</div>
		  </td>
	   	<td width="600px;" class="login_video">	  	
				<iframe width="600" height="360" src="http://www.youtube.com/embed/eeu90xAEMB8" frameborder="0" allowfullscreen></iframe>
			</td>
	  </tr>
	</table>  			
</div>